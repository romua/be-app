{
  "name": "circle-ci-test",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "fe:start:dev": "ng serve",
    "fe:build:prod": "ng build --stats-json --configuration=production",
    "fe:test": "ng test --watchAll",
    "fe:test:ci": "ng test --collect-coverage",
    "fe:build:ci": "ng test && ng build --aot",
    "fe:lint": "ng lint",
    "fe:e2e": "fe/cypress open",
    "fe:check": "npm run fe:lint && npm run fe:test:ci",
    "sonar": "sonar-scanner",
    "fe:stat-es2015": "webpack-bundle-analyzer dist-fe/hooks/stats-es2015.json",
    "fe:stat-es5": "webpack-bundle-analyzer dist-fe/hooks/stats-es5.json",
    "separator_": "",
    "_separator": "",
    "be:build:prod": "webpack --config be/webpack.config.js --progress",
    "be:build:dev": "webpack --config be/webpack.config.js --watch",
    "be:start:dev": "nodemon dist/app.js",
    "be:start:prod": "pm2 start dist/app.js",
    "be:prod": "npm run be:build:prod && npm run be:start:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "lodash": "^4.17.13",
    "reset-css": "^4.0.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.0.3",
    "@angular-devkit/build-angular": "^0.801.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/dotenv": "^6.1.1",
    "@types/jest": "^24.0.15",
    "@types/node": "^10.12.10",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^5.0.0",
    "copy-webpack-plugin": "^5.0.3",
    "cypress": "^3.3.1",
    "jest": "^24.8.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^1.19.1",
    "sonar-scanner": "^3.1.0",
    "style-resources-loader": "^1.2.1",
    "ts-loader": "^6.0.4",
    "ts-node": "^7.0.1",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.5"
  }
}
